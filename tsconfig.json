{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "baseUrl": "./src",
    "sourceMap": false,
    "noImplicitAny": true,
    // "strictNullChecks": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude":[
    "node_modules"
  ]
}
