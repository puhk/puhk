{
  "name": "@nojball/client-ui",
  "version": "0.0.1",
  "license": "MIT",
  "main": "build/cjs/index.js",
  "module": "build/es6/index.js",
  "types": "build/es6/index.d.ts",
  "scripts": {
    "start": "yarn run server:dev",
    "build": "yarn build:es6 && yarn build:cjs",
    "build:cjs": "tsc --outDir build/cjs --declaration --module commonjs",
    "build:es6": "tsc --outDir build/es6 --declaration",
    "server:dev": "cross-env NODE_ENV=development yarn run webpack-dev-server -- --inline --progress --profile --watch"
  },
  "dependencies": {
    "color": "^1.0.3",
    "core-decorators": "^0.19.0",
    "prop-types": "^15.5.10",
    "react": "^16.4.1",
    "react-contextmenu": "^2.5.2",
    "react-dom": "^16.4.1",
    "resize-observer-polyfill": "^1.3.2",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@types/color": "^1.0.3",
    "@types/core-decorators": "^0.10.32",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/node": "^8.0.0",
    "@types/react": "^16.4.3",
    "@types/react-dom": "^16.0.6",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "^2.2.16",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.25.0",
    "node-sass": "^4.1.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "ts-node": "^3.0.6",
    "typescript": "^2.9.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
