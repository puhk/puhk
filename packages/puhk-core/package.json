{
  "name": "@puhk/puhk-core",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "main": "build/cjs/index.js",
  "module": "build/es6/index.js",
  "types": "build/es6/index.d.ts",
  "scripts": {
    "build:dist": "cross-env NODE_ENV=production webpack --progress --colors",
    "build:cjs": "tsc --module commonjs --outDir build/cjs --declaration",
    "build:es6": "tsc --outDir build/es6 --declaration",
    "start": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --profile --watch",
    "test": "jest"
  },
  "dependencies": {
    "core-decorators": "^0.19.0",
    "eventemitter3": "^2.0.3",
    "immutability-helper": "^2.9.0",
    "lodash": "^4.17.3",
    "mainloop.js": "^1.0.3",
    "peerjs": "^1.0.0",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.63",
    "@types/mainloop.js": "^1.0.5",
    "@types/victor": "^1.1.0",
    "awesome-typescript-loader": "^3.1.3",
    "husky": "^4.0.7",
    "jest": "^24.9.0",
    "jest-ts-auto-mock": "^1.0.10",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^24.2.0",
    "ttypescript": "^1.5.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "useTabs": true,
    "singleQuote": true,
    "printWidth": 120
  }
}
