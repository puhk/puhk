{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "baseUrl": "./",
    "sourceMap": false,
    "strict": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "plugins": [
      {
        "transform": "ts-auto-mock/transformer",
        "cacheBetweenTests": false
      }
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude":[
    "node_modules"
  ]
}
